{
  "name": "data-selfie-scraper",
  "version": "0.0.1",
  "description": "Instagram and SLNSW scraper.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/slnsw/data-selfie-scraper.git"
  },
  "main": "build/app.js",
  "scripts": {
    "start": "node build/app.js",
    "dev": "webpack --watch & node-dev --no-notify --no-deps -r dotenv/config build/app.js",
    "scrape-slnsw": "npx babel-node ./src/scrape.js slnsw",
    "deploy": "serverless --stage staging deploy",
    "build": "webpack --progress",
    "test": "node -r dotenv/config ./node_modules/.bin/jest --forceExit --runInBand",
    "test:watch": "node -r dotenv/config ./node_modules/.bin/jest --watch",
    "invoke": "sls invoke local -f graphql --stage staging"
  },
  "keywords": [],
  "author": "Kaho Cheung",
  "license": "ISC",
  "dependencies": {
    "apollo-cache-inmemory": "^1.1.12",
    "apollo-client": "^2.2.8",
    "apollo-link": "^1.2.1",
    "apollo-link-http": "^1.5.3",
    "aws-sdk": "^2.7.1",
    "aws-serverless-express": "3.0.2",
    "axios": "^0.16.2",
    "babel-cli": "^6.26.0",
    "bluebird": "^3.5.0",
    "body-parser": "^1.15.2",
    "cheerio": "^1.0.0-rc.2",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-only-warn": "^1.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "express": "^4.14.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.8.0",
    "node-fetch": "^2.1.2",
    "prettier": "^1.11.1",
    "query-string": "^5.0.0",
    "request": "^2.83.0",
    "request-promise": "^4.2.1",
    "wpapi": "^1.1.2"
  },
  "devDependencies": {
    "@types/aws-lambda": "0.0.5",
    "@types/aws-serverless-express": "0.0.2",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^4.4.1",
    "eslint-config-airbnb-base": "^11.3.1",
    "@slnsw/eslint-config-dxlab": "0.0.9",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "jest": "^22.4.3",
    "node-dev": "^3.1.3",
    "serverless-offline": "3.15.3",
    "serverless-plugin-include-dependencies": "^2.1.2",
    "webpack": "^3.5.5",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "./build"
    ]
  }
}
